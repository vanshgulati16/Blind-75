class Solution:
    def deleteMiddle(self, head: Optional[ListNode]) -> Optional[ListNode]:
        fast = head
        slow = head
        prev = None

        if head is None or head.next is None:
            return
        while fast is not None and fast.next is not None:
            fast= fast.next.next
            prev = slow
            slow = slow.next
        prev.next = slow.next

        return head 
